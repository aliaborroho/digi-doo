<div>
<h1>Your Inner Circle</h1>
</div>

{{#link-to 'digidoo.contacts.create' class='btn btn-default'}}Add new contact to your inner circle{{/link-to}}

<ul style="width:100%">

    {{#each model as |contact|}}
        {{#unless contact.isNew}}
            <li>{{contact.fullName}} - {{contact.phone}}</li>
        {{/unless}}
    {{/each}}
</ul>

{{outlet}}
