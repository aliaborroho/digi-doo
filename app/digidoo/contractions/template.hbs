
<table>
    <tr> <td>{{startTime}}</td><td>{{endTime}}</td></tr>
   <h2>Contraction Timer</h2>
<div>
    <div align="center"><button class="stopwatch hvr-pulse" value="stopped">00:00:00</button></div>
    <div class="col-md-offset-4">
    </div>

    <table class="table panel results hidden"></table>
</div>
<div align="center">
    <h1>Results Table</h1>
    <table class="table panel">
        <tr>
            <th>
                Contraction Duration
            </th>
            <th>Time between measured contractions</th>
        </tr>
    </table>
</div>





</table>
<div class="wrapper">
    <h1 id="decision"></h1>
    <button id="theButton" onclick="theButton();">Contraction Ended (start)</button> &nbsp;&nbsp;&nbsp; <button id="reset" onclick="reset();">Reset</button>
    <p>Time Lapsed <span id="timer"></span></p>
    <p id="timesLogged"></p>
</div>
